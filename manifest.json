{
    "name": "Volume Booster",
    "version": "1.0",
    "description": "Boost the volume of audio and video on web pages.",
    "manifest_version": 3,
    "author": "Avasty",
    "permissions": [
        "activeTab",
        "https://*/*",
        "http://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
            "https://*/*",
            "http://*/*"
            ],
        "js": ["content.js"]
        }
    ]
}